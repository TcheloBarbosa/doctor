        <!-- Define the action to show the tree view -->
        <record model="ir.actions.act_window" id="action_pacientes_contas_procedimentos_tree">
            <field name="name">Pacientes Contas Procedimentos (Tree View)</field>
            <field name="res_model">pacientes.contas.procedimentos</field>
            <field name="view_mode">tree</field>
            <field name="view_id" ref="view_pacientes_contas_procedimentos_tree"/>
            <field name="context">{'show_invisible': 1}</field>
        </record>

        <!-- Define the action to show the form view -->
        <record model="ir.actions.act_window" id="action_pacientes_contas_procedimentos_form">
            <field name="name">Pacientes Contas Procedimentos (Form View)</field>
            <field name="res_model">pacientes.contas.procedimentos</field>
            <field name="view_mode">form</field>
            <field name="view_id" ref="view_pacientes_contas_procedimentos_form"/>
            <field name="target">new</field>
            <!-- Set this form view as the default view for the model -->
            <field name="context">{"form_view_ref": "view_pacientes_contas_procedimentos_form"}</field>
        </record>

        <!-- Define the menu item to access the views -->
