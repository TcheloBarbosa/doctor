<odoo>
    <data>
        <record id="view_doctor_form" model="ir.ui.view">
            <field name="name">doctor.form</field>
            <field name="model">doctor</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="id"/>
                            <field name="nome"/>
                            <field name="senha"/>
                            <field name="alinhamento"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>
